import React, { Component } from 'react';
class  classClick extends Component {
    state = {  }
    clickHandler= ()=>{
        console.log('Clicked the button')
    } 
    render() { 
        return (
            <div>
                <button onClick={this.clickHandler}>
                    Click Me
                </button>
            </div>
        );
    }
}
 
export default classClick;
